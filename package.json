{
	"name": "@fen95/ts-module-template",
	"version": "1.0.0",
	"description": "node module boilerplate",
	"engines": {
		"node": ">= 10.13 <13"
	},
	"repository": "",
	"main": "dist/main.js",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^9.1.2",
		"@commitlint/config-conventional": "^9.1.1",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@types/jest": "^26.0.7",
		"@types/node": "^14.0.27",
		"@typescript-eslint/eslint-plugin": "^3.7.1",
		"@typescript-eslint/parser": "^3.7.1",
		"eslint": "^7.5.0",
		"husky": "^4.2.5",
		"jest": "~26.1.0",
		"lint-staged": "^10.2.11",
		"nodemon": "^2.0.4",
		"prettier": "~2.0.5",
		"rimraf": "^3.0.2",
		"semantic-release": "^17.1.1",
		"ts-jest": "^26.1.4",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.7"
	},
	"resolutions": {
		"dot-prop": "^5.1.1"
	},
	"files": [
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"start": "DEPLOY_ENV=local nodemon --watch 'src/**/*' -e ts --exec ts-node --log-error --files ./src/main.ts",
		"clean": "rimraf coverage dist tmp",
		"build": "rimraf dist && tsc  --module commonjs --outDir dist",
		"build:watch": "tsc -w  --module commonjs --outDir dist",
		"lint": " eslint ./src --ext .js,.jsx,.ts,.tsx",
		"test": "jest --coverage",
		"ddd": "prettier --write",
		"test:watch": "jest --watch",
		"publish": "HUSKY_SKIP_HOOKS=1 semantic-release --ci false"
	},
	"license": "MIT",
	"dependencies": {}
}
